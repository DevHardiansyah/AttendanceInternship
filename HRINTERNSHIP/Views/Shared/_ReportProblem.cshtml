    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.css">
    <link rel="stylesheet" href="https://cdn.datatables.net/datetime/1.1.0/css/dataTables.dateTime.min.css">

    <style>
        .red {
            color: #ff2a00
        }

        .orange {
            color: #ff5900
        }

        .yellow {
            color: #ffee00;
        }

        .purple {
            color: #e3308f;
        }

        .high_purple {
            color: #6200d9;
        }

        .blue_sky {
            color: #0089d9;
        }
    </style>
    <link href="~/Content/css/app.css" rel="stylesheet" />
</head>
<body>
    <div class="row">
        <div class="col-md-12">
            <div class="card">
                <div class="card-body">
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>
    <footer class="footer d-flex flex-column flex-md-row align-items-center justify-content-between ">
        <p class="text-muted text-center text-md-left ">Copyright © 2021 <a href=" " target="_blank ">HRD</a>. All rights reserved</p>
        <p class="text-muted text-center text-md-left mb-0 d-none d-md-block ">Handcrafted With <i class="mb-1 text-primary ml-1 icon-small "></i></p>
    </footer>

    <!-- base js -->
    <script src="~/Scripts/js/app.js "></script>
    <script src="~/Content/plugins/feather-icons/feather.min.js "></script>
    <script src="~/Content/plugins/perfect-scrollbar/perfect-scrollbar.min.js "></script>
    <script src="~/Content/plugins/dropzone/dropzone.min.js"></script>
    <script src="~/Scripts/js/template.js "></script>
