    <link href="~/Content/plugins/flag-icon-css/css/flag-icon.min.css" rel="stylesheet" />
    <link href="~/Content/plugins/perfect-scrollbar/perfect-scrollbar.css" rel="stylesheet" />
    <link href="~/Content/plugins/dropzone/dropzone.min.css" rel="stylesheet" />
