    <script>
        //counting intern
        $.ajax({
            type: "POST",
            url: "/Intern/CountAttendance",
            dataType: "json",
            success: function (Data) {
                document.getElementById("total_hour").innerHTML = Data.total_hour;
                document.getElementById("total_atn").innerHTML = Data.total_atn;
                document.getElementById("total_abs").innerHTML = Data.total_abs;
                document.getElementById("total_late").innerHTML = Data.total_late;
                document.getElementById("total_report").innerHTML = Data.total_report;
            }
        });
        $.ajax({
            url: '/Intern/AttendanceInternIn',
            type: "GET",
            dataType: "JSON",

            success: function (result) {
                var AttendanceInternIn = [];

                $.each(result, function (i, data) {
                    AttendanceInternIn.push({
                        backgroundColor: '#262261',
                        borderColor: '#10b759',
                        textColor: '#fff',
                        id: data.Attendance_id,
                        start: data.Entry_Date_Time,
                        end: data.Entry_Date_Time,
                        title: 'Clock In',
                        description: data.Entry_Date_Time,
                        activity: data.Activity
                    });
                });
                var AttendanceInternOut = [];
                $.each(result, function (i, data) {
                    AttendanceInternOut.push({
                        backgroundColor: '#e81d2c',
                        borderColor: '#10b759',
                        textColor: '#fff',
                        id: data.Expr2,
                        start: data.Entry_Out,
                        end: data.Entry_Out,
                        title: 'Clock Out',
                        description: data.Entry_Out,
                        activity: data.Activity
                    });
                });
                var PublicHoliday = [];
                $.each(result, function (i, data) {
                    PublicHoliday.push({
                        backgroundColor: 'rgba(253,126,20,.25)',
                        borderColor: '#fd7e14',
                        id: data.id,
                        start: data.Date_public_holidays,
                        end: data.Date_public_holidays,
                        title: data.Date_description,
                        description: data.Date_mandatory,
                        activity: data.Date_description
                    });
                });
                // Birthday Events Source

                // initialize the calendar
                $('#attendanceintern').fullCalendar({
                    header: {
                        left: 'prev,today,next',
                        center: 'title',
                        right: 'month,agendaWeek,agendaDay,listMonth'
                    },
                    editable: true,
                    droppable: true, // this allows things to be dropped onto the calendar
                    dragRevertDuration: 0,
                    defaultView: 'month',
                    eventLimit: true, // allow "more" link when too many events
                    eventSources: [AttendanceInternIn, AttendanceInternOut, PublicHoliday],
                    eventClick: function (event, jsEvent, view) {
                        $('#modalTitle1').html(event.title);
                        $('#modalBody1').html(event.description);
                        $('#activity').html(event.activity);
                        $('#fullCalModal').modal();
                    },
                    dayClick: function (date, jsEvent, view) {
                        // for show form createv (modal)
                        $("#createEventModal").modal("show");
                    },
                });


                var isEventOverDiv = function (x, y) {
                    var external_events = $('#external-events');
                    var offset = external_events.offset();
                    offset.right = external_events.width() + offset.left;
                    offset.bottom = external_events.height() + offset.top;

                    // Compare
                    if (x >= offset.left &&
                        y >= offset.top &&
                        x <= offset.right &&
                        y <= offset.bottom) { return true; }
                    return false;
                }


            }
        });
    </script>
    
